<div class = "container">
      <div class= "category">
        <% @categories.each do |category| %>
          <%= link_to category.title, category_path(category) %>
          <% end %>
      </div>

      <div class = "item">
        <% @items.each do |item| %>
          <p><%= item.title %> | $<%= item.price.to_f/100 %> | <%= button_to 'Add to Cart', line_items_path(item_id: item) %></p>
        <% end %>
      </div>




</div>
